alt_send_esc: true
bell:
  animation: EaseOutExpo
  color: '0xffffff'
  duration: 0
colors:
  bright:
    black: '0x686868'
    blue: '0x59C2FF'
    cyan: '0x95E6CB'
    green: '0xC2D94C'
    magenta: '0xFFEE99'
    red: '0xF07178'
    white: '0xFFFFFF'
    yellow: '0xFFB454'
  cursor:
    cursor: '0x878787'
    text: '0xeeeeee'
  dim:
    black: '0x31363b'
    blue: '0x1b668f'
    cyan: '0x186c60'
    green: '0x17a262'
    magenta: '0x614a73'
    red: '0x783228'
    white: '0x63686d'
    yellow: '0xb65619'
  indexed_colors: []
  normal:
    black: '0x01060E'
    blue: '0x53BDFA'
    cyan: '0x90E1C6'
    green: '0x91B362'
    magenta: '0xFAE994'
    red: '0xEA6C73'
    white: '0xC7C7C7'
    yellow: '0xF9AF4F'
  primary:
    background: '0x0A0E14'
    bright_background: '0x000000'
    bright_foreground: '0xffffff'
    dim_background: '0x31363b'
    dim_foreground: '0xeff0f1'
    foreground: '0xB3B1AD'
  search:
    focused_match:
      background: CellForeground
      foreground: CellBackground
    matches:
      background: '0xb26818'
      foreground: '0xb2b2b2'
  selection:
    background: '#3e4451'
    text: CellForeground
cursor:
  blink_interval: 250
  style:
    blinking: Always
  unfocused_hollow: true
draw_bold_text_with_bright_colors: true
